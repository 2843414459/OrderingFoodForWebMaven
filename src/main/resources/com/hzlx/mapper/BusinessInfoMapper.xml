<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzlx.mapper.BusinessInfoMapper">
    <resultMap id="result"  type="businessInfo">
        <id column="id" property="id"></id>
        <result column="name" property="name"></result>
        <result column="user_name" property="userName"></result>
        <result column="password" property="password"></result>
        <result column="address" property="address"></result>
        <result column="tel" property="tel"></result>
        <result column="create_time" property="createTime"></result>
        <result column="job_type" property="jobType"></result>
        <result column="status" property="status"></result>
    </resultMap>
    <select id="selectBusinessInfoByUserNameAndPwd" resultType="com.hzlx.entity.BusinessInfo">
        select * from t_business_info where user_name=? and `password`=#{id}
    </select>
</mapper>